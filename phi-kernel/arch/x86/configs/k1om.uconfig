# minimum sane bootable configuration
CONFIG_SMP=y
CONFIG_X86_EXTENDED_PLATFORM=y
CONFIG_X86_MICPCI=y
CONFIG_MK1OM=y
CONFIG_NR_CPUS=255
CONFIG_SCHED_SMT=y
CONFIG_SCHED_MC=y
CONFIG_SFI=y
CONFIG_MIC_PM=y
CONFIG_MIC_CPUIDLE=y
CONFIG_MIC_CPUFREQ=y
CONFIG_LOCK_KERNEL=y
CONFIG_BKL=y
CONFIG_TRANSPARENT_HUGEPAGE=y
CONFIG_TRANSPARENT_HUGEPAGE_ALWAYS=y
CONFIG_PARALLEL_AP_BOOT=y
